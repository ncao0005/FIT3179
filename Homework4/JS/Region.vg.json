{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 800,
    "height": 600,
    "title": "Number of flights taht port at different region",
    "data": {
      "url": "https://raw.githubusercontent.com/ncao0005/FIT3179/main/Homework4/Different%20region.csv"
    },
     "transform": [
       {"filter":{"field": "Month","range":[{"year": 2018,"month":1,"date": 1},{"year": 2018,"month":12,"date": 1}]}},
       {
         "aggregate": [{
           "op": "sum",
           "field": "Allflights",
           "as":"Totalflights"
         }],
         "groupby": ["PortRegion"]
       }
    ],
   "encoding": {
      "x": {
        "field": "Month",
        "type": "ordinal",
        "title": "Date"
      },
      "y": {
        "field": "Allflights",
        "type": "quantitative",
        "title":"Total number of flights"
      }
    },
  
}