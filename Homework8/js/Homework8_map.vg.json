{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 800,
    "height": 450,
    "title": "Australia International Airlines Operated Flights",
    "layer": [
      {
        "data": {
          "url": "https://raw.githubusercontent.com/JiazhouLiu/FIT3179/main/VegaLite/2_symbol_map/js/ne_110m_admin_0_countries.topojson",
          "format": {"type": "topojson", "feature": "ne_110m_admin_0_countries"}
        },
        "mark": {"type": "geoshape", "fill": "lightgray", "stroke": "white"}
      },
      {
        "data": {
          "url": "https://raw.githubusercontent.com/ncao0005/FIT3179/main/Homework9.csv"
        },
          "transform":[
            {"filter":
              {"field": "Australia", "equal": "Melbourne"}}
          ],
          "projection": {"type": "equalEarth"},
          "mark": "circle",
          "encoding": {
            "longitude": {
              "field": "LongitudeInAustralia",
              "type": "quantitative"
          },
            "latitude": {
              "field": "LatitudeInAustralia",
              "type": "quantitative"
          },
          "size": {"value": 20},
          "color": {"value": "#ff7f00"},
          "tooltip":[{"field":"Australia","type": "nominal", "title":"City name" }]
        }
      },
      {
        "data": {
          "url": "https://raw.githubusercontent.com/ncao0005/FIT3179/main/Homework9.csv"
        },
        "projection": {"type": "equalEarth"},
        "mark": "circle",
        "encoding": {
          "longitude": {
            "field": "LongitudeInternation",
            "type": "quantitative"
          },
          "latitude": {
            "field": "LatitudeInternational",
            "type": "quantitative"
          },
          "size": {"value": 20},
          "color": {"value": "#377eb8"},
          "tooltip":[{"field":"International","type": "nominal", "title":"City name" }]
        }
      },
      {
        "data": {
          "url": "https://raw.githubusercontent.com/ncao0005/FIT3179/main/Homework9.csv"
        },
        "transform":[
            {"filter":
              {"field": "Australia", "equal": "Melbourne"}}
        ],
        "projection": {"type": "equalEarth"},
        "mark": "rule",
        "encoding": {
          "longitude": {
            "field": "LongitudeInternation",
            "type": "quantitative"
          },
          "latitude": {
            "field": "LatitudeInternational",
            "type": "quantitative"
          },
          "longitude2": {"field": "LongitudeInternation"},
          "latitude2": {"field": "LatitudeInternational"}
        }
      }
    ]
